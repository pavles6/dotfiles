{
  "theme": "One Dark",
  "terminal": {
    "dock": "right",
    "font_family": "IosevkaTerm Nerd Font"
  },
  "tabs": {
    "file_icons": true,
    "git_status": true
  },
  "project_panel": {
    "dock": "right",
    "scrollbar": {
      "show": "always"
    }
  },
  "languages": {
    "Python": {
      "wrap_guides": [80],
      // "format_on_save": {
      //   "language_server": {
      //     "name": "ruff"
      //   }
      // },
      // "formatter": {
      //   "language_server": {
      //     "name": "ruff"
      //   }
      // },
      "language_servers": ["pyright"]
    },
    "YAML": {
      "format_on_save": "off"
    }
  },
  "soft_wrap": "editor_width",
  "buffer_font_features": {
    "calt": false
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "confirm_quit": true,
  "vertical_scroll_margin": 10,
  "vim_mode": true,
  "ui_font_size": 20,
  "vim": {
    "use_system_clipboard": "never"
  },
  "buffer_font_size": 20,
  "buffer_font_family": "IosevkaTerm Nerd Font",
  "ui_font_family": "Iosevka Aile",
