{
  "indent_guides": {
    "enabled": true
  },
  "theme": "Base16 iA Dark",
  "terminal": {
    "dock": "right",
    "font_family": "IosevkaTerm Nerd Font"
  },
  "project_panel": {
    "dock": "right"
  },
  "experimental.theme_overrides": {
    "terminal.ansi.red": "#d86868",
    "syntax": {
      "operator": {
        "color": "#b98eb2"
      }
    }
  },
  "languages": {
    "Python": {
      "wrap_guides": [80]
    }
  },
  "soft_wrap": "editor_width",
  "buffer_font_features": {
    "calt": false
  },
  "confirm_quit": true,
  "vim_mode": true,
  "ui_font_size": 20,
  "buffer_font_size": 20,
  "scroll_sensitivity": 2,
  "buffer_font_family": "Iosevka Term",
  "ui_font_family": "Iosevka Aile",
  "relative_line_numbers": true
}
