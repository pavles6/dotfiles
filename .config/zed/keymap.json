[
  {
    "context": "vim_mode == visual",
    "bindings": {
      "shift-s": [
        "vim::PushOperator",
        {
          "AddSurrounds": {}
        }
      ]
    }
  },
  {
    "context": "Editor && inline_completion && !showing_completions",
    "bindings": {
      "ctrl-f": "editor::AcceptInlineCompletion",
      "tab": "editor::Tab"
    }
  }
]
