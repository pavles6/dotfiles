[
  {
    // global overrides
    "bindings": {
      "cmd-shift-f": null,
      "cmd-shift-p": null,
      "cmd-shift-r": null,
      "cmd-shift-o": null,
      "cmd-shift-e": null,
      "cmd-s": null,
      "shift-p": null,
      "shift-n": null
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "ctrl-w": null,
      "ctrl-n": null,
      "ctrl-p": null,
      "ctrl-t": null
    }
  },
  {
    "context": "vim_mode == visual",
    "bindings": {
      "shift-s": ["vim::PushAddSurrounds", {}]
    }
  },
  {
    "context": "VimControl && !menu && vim_mode != operator",
    "bindings": {
      "w": "vim::NextSubwordStart",
      "b": "vim::PreviousSubwordStart",
      "e": "vim::NextSubwordEnd",
      "g e": "vim::PreviousSubwordEnd"
    }
  },
  {
    "context": "Editor && edit_prediction && !showing_completions",
    "bindings": {
      "ctrl-f": "editor::AcceptEditPrediction",
      "tab": "editor::Tab"
    }
  },
  {
    "context": "GitPanel || ProjectPanel || CollabPanel || OutlinePanel || ChatPanel || VimControl || EmptyPane || SharedScreen || MarkdownPreview || KeyContextView",
    "bindings": {
      "cmd-r": "workspace::ToggleRightDock",
      "alt-r": "workspace::ToggleRightDock",
      "cmd-b": "workspace::ToggleLeftDock",
      "alt-b": "workspace::ToggleLeftDock",
      "space p": "file_finder::Toggle",
      "space f": [
        "task::Spawn",
        {
          "task_name": "file_finder",
          "reveal_target": "center"
        }
      ],
      "space g": "git_panel::ToggleFocus",
      "space e": "project_panel::ToggleFocus",
      "space a": "assistant::ToggleFocus",
      "space o": "outline::Toggle",
      "space x": "zed::Extensions",
      "space ,": "tab_switcher::Toggle"
    }
  },
  {
    "context": "Editor && vim_mode == visual",
    "bindings": {
      "shift-n": "editor::MoveLineDown",
      "shift-p": "editor::MoveLineUp",
      "ctrl-a": "editor::SelectAll"
    }
  }
]
